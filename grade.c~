// In grade.c
#include <stdio.h>

int main(void) {
    int grade = 77 /* arbitrary initialiaztion of grade */;
    char letter = 'f';    /* by default... */

    if (grade > 90) {
        letter = 'a';
    }
    if (grade > 80) {
        letter = 'b';
    }
    if (grade > 70) {
                                                      letter = 'c';
    }
    if (grade > 60) {
        letter = 'd';
    }
    printf("%c\n", letter);
    return 0;
}
